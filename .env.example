# Root env file used by Docker Compose for variable substitution.
# Copy to .env and fill in secrets:
#   cp .env.example .env

# Shared secret to authenticate the scraper -> server upload.
SCRAPER_SECRET=change_me

# Secret salt for analytics hashing (server).
ANALYTICS_HASH_SALT=change_me

# TMDB API key used by the scraper for metadata enrichment.
TMDB_API_KEY=
